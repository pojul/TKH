}

.cell_active {
  background-color: #ececec;
}

.media-box__hd_in-appmsg {
  margin: 20rpx 20rpx 28rpx 20rpx;
  width: 186rpx;
  height: 186rpx;
  display: flex;
  position: relative;
  overflow: hidden;
}

.media-box__bd_in-appmsg {
  -webkit-box-flex: 1;
