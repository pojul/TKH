    padding-bottom: 33rpx;
    position: relative;
}

.waybill-left {
    color: #606060;
    font-size: 24rpx;
    margin-left: 46rpx;
    margin-right: 118rpx;
}

.express-icon {
    width: 32rpx;
    height: 32rpx;
    position: absolute;
    left: 0rpx;
}

.waybill-btn {
    border: 1rpx solid rgba(38,188,197,0.4);
    border-radius: 30rpx;
    font-size: 22rpx;
    display: initial;
    color: #26BCC5;
    position: absolute;
    right: 0rpx;
    top: 20rpx;
    line-height: 42rpx;
    width: 100rpx;
    height: 42rpx;
    text-align: center;
    bottom: 0;
}

.btn-pre {
    text-align: center;
}

.btn-change {
    
    color: #fff;
    border-radius: 100rpx;
    width: 360rpx;
    height: 78rpx;
    font-size: 32rpx;
    line-height: 78rpx;
    margin: 22rpx 0 39rpx 0;

    display: inline-block;
}

.container {
    background: #fff;
    left: 0;
    top: 0;
    position: absolute;
    height: 100%;
    width: 100%;
}

.btn-hover {
    background-color: #81b487;
}

.img-box {
    width: 100%;
    margin-top: 37.48%;
    text-align: center;
}

.box-image {
    width: 415rpx;
    height: 218rpx;
    display: -webkit-inline-box;
}

.img-text {
    font-size: 32rpx;
    display: block;
}

.btn-pre {
    text-align: center;
}

.view-hover {
    background-color: #909090;
}

.public-card {
    padding-bottom: 26rpx;
}

.piblic-content {
    margin: 22rpx 30rpx 0 21.5rpx;
    position: relative;
    display: flex;
    background: #FFFBE2;
    flex-direction: column;
}

.public-img {
    width: 32rpx;
    height: 32rpx;
    padding-left: 8.5rpx;
    padding-right: 16.5rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto 0;
}

.public-text {
    margin: 0rpx 6rpx 0rpx 52rpx;
    color: #8B572A;
    line-height: 38rpx;
    font-size: 24rpx;
    letter-spacing: -0.01rpx;
}

.text-undeline {
    text-decoration: underline;
}

.btn {
    margin: 32rpx auto;
    width: 240rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: white;
    font-size: 30rpx;
    text-align: center;
    background-image: linear-gradient(-90deg,#1DD49E 2%,#26BCC5 97%);
    box-shadow: 0 6px 10px -2px rgba(37,191,193,0.50);
    border-radius: 50px;
}

.media-box {
    position: relative;
}

.media-box_appmsg {
    display: flex;
    width: 100%;
}

.cell_active {
    background-color: #ececec;
}

.media-box__hd_in-appmsg {
    margin: 20rpx 20rpx 28rpx 20rpx;
    width: 186rpx;
    height: 186rpx;
    display: flex;
    position: relative;
    overflow: hidden;
}

.media-box__bd_in-appmsg {
    -webkit-box-flex: 1;
    flex: 1;
    min-width: 0;
    margin: 31rpx 21rpx 29rpx 0rpx;
    position: relative;
}

.media-box__thumb {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto auto;
}

.media-box__title {
    font-weight: 500;
    font-size: 28rpx;
    line-height: 40rpx;
    width: auto;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-bottom: 20rpx;
}

.media-box__content {
    position: absolute;
    bottom: 42rpx;
}

.text-pre {
    font-size: 24rpx;
    color: #999999;
    text-decoration: line-through;
}

.media-box__desc {
    color: #666666;
    font-size: 28rpx;
    display: -webkit-box;
    position: absolute;
    bottom: 0;
    align-content: flex-end;
}

.text-orangered {
    color: #26BCC5;
    font-size: 42rpx;
    font-family: DIN Alternate;
}

.text-tag {
    font-size: 22rpx;
    color: #FC8A1D;
    border-radius: 2rpx;
    border-color: rgba(252,138,29,0.24);
    background-color: rgba(252,138,29,0.1);
    padding: 5rpx 7rpx 5rpx 7rpx;
    margin-right: 4rpx;
}

.bottom_right {
    font-size: 24rpx;
    font-weight: 300;
    color: #999999;
    margin-top: 16rpx;
    position: absolute;
    right: 0rpx;
    top: 125rpx;
}

.attract {
    color: #26A0BD;
    font-weight: bold;
    font-family: DIN Alternate;
}

.newusertag {
    background-size: 100%;
    width: 70rpx;
    height: 70rpx;
    z-index: 10;
}