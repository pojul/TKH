  bottom: 0;
  left: 0;
  margin: auto auto;
}

.media-box__title {
  font-weight: 500;
  font-size: 28rpx;
  line-height: 40rpx;
  width: auto;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 20rpx;
}

.media-box__content {
  position: absolute;
  bottom: 42rpx;
}

.text-pre {
