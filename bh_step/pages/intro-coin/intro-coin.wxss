page {
    width: 100%;
    height: 100%;
}

.main {
    width: 100%;
    height: 100%;
}

.head {
    width: 750rpx;
    height: 170rpx;
    line-height:170rpx;
    position: relative;
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.head .title {
  font-size:22px;
  color:#fff;
  text-shadow:-1px 3px 3px #035737;
  font-family:PingFangSC-Semibold;
  position:relative;
  text-align: center;
}
.intro-content {
  padding:0 56rpx 30rpx;
  font-size:26rpx;
}

.intro-content .left-title {
  margin:50rpx 0 20rpx;
  font-weight: 600;
}
.intro-content .center-title {
  position: relative;
  width: 180rpx;
  text-align: center;
  margin: 50rpx auto 20rpx;
  font-weight: 600;
  font-size: 30rpx;
}

.intro-content .center-title::after {
  background-color: #00AF64;
  height: 3px;
  content: "";
  width: 60rpx;
  border-radius: 6rpx;
  position: absolute;
  bottom: -10rpx;
  left: 58rpx;
}

.intro-content .p {
  line-height: 2;
  margin-top:16rpx;
}

.intro-content .p2 {
  line-height: 2;
  margin-top:0rpx;
}
